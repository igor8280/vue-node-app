{
  "name": "node-skeleton",
  "version": "0.0.1",
  "description": "RESTful API Project",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "NODE_ENV=production pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "export NODE_ENV=test && mocha",
    "test-watch": "nodemon --exec 'npm test'"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Igor Gojkovic <igor.gojkovic@team4js.com>, Milos Markovic <milos.markovic@team4js.com>, Andrija Gojkovic <andrija.gojkovic@team4js.com>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.1.6",
    "mongoose-aggregate-paginate": "^1.1.2",
    "multer": "^1.3.1",
    "node-watch": "^0.5.8",
    "pm2": "^3.0.0",
    "sharp": "^0.20.5",
    "swagger-ui-express": "^3.0.10",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "expect": "^23.4.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "supertest": "^3.1.0"
  }
}
